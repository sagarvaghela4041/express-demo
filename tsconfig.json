{
  "compilerOptions": {
    "outDir": "dist",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}